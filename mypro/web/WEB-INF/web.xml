<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>StudentTypeServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.StudentTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginJudgeServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.LoginJudgeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherTypeServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.TeacherTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClassTypeServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.ClassTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChooseSubjectServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.ChooseSubjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectTypeServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.SubjectTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentIndexServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.StudentIndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateScoreServlet</servlet-name>
        <servlet-class>cn.mypro.servlet.UpdateScoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentTypeServlet</servlet-name>
        <url-pattern>/studenttype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginJudgeServlet</servlet-name>
        <url-pattern>/loginjudge</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherTypeServlet</servlet-name>
        <url-pattern>/teachertype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClassTypeServlet</servlet-name>
        <url-pattern>/classtype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChooseSubjectServlet</servlet-name>
        <url-pattern>/choosesubject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectTypeServlet</servlet-name>
        <url-pattern>/subjecttype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentIndexServlet</servlet-name>
        <url-pattern>/studentindex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateScoreServlet</servlet-name>
        <url-pattern>/updatescore</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>sys/login.jsp</welcome-file>
    </welcome-file-list>
</web-app>